{
  "default_backend": "hybrid",
  "auto_optimization": true,
  "monitoring_enabled": true,
  "backends": {
    "hybrid": {
      "cache_size_mb": 100,
      "max_entries": 10000,
      "ttl_seconds": 3600
    },
    "redis": {
      "host": "localhost",
      "port": 6379,
      "db": 0
    },
    "sqlite": {
      "persistent_dir": "./output/memory"
    }
  },
  "performance_monitoring": {
    "log_interval_seconds": 300,
    "alert_thresholds": {
      "low_hit_ratio": 0.5,
      "high_memory_usage_mb": 500
    }
  }
}