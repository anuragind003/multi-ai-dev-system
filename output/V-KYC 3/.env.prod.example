# .env.prod.example - For production (docker-compose.prod.yml or deployed environment)

# Application Settings
APP_ENV=production
CORS_ORIGINS=https://your-production-domain.com

# Database Settings (PostgreSQL)
POSTGRES_USER=produser
POSTGRES_PASSWORD=secure_prod_password_change_me
POSTGRES_DB=proddb
DB_HOST=your_rds_endpoint_or_internal_ip # e.g., my-prod-db.abcdef.us-east-1.rds.amazonaws.com
DB_PORT=5432

# Grafana Settings (Optional, for initial setup)
GF_SECURITY_ADMIN_USER=admin
GF_SECURITY_ADMIN_PASSWORD=secure_grafana_password_change_me